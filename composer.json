	"description": "PHP PSR-7 compatible HTTP client (using cURL)",
	"keywords": ["http", "client", "request", "response"],
	"authors": [{"name": "Jérémy 'Jejem' Desvages", "email": "jejem@phyrexia.org"}],
	"license": "MIT",
	"require": {
		"php": "^5.3 || ^7.0 || ^8.0",
		"ext-curl": "*",
		"psr/http-message": "^1.0",
		"guzzlehttp/psr7": "^1.0"
	},
	"provide": {
		"psr/http-message-implementation": "1.0"
	},
	"autoload": {
		"psr-4": {"Phyrexia\\Http\\": "src/"}
	}
}
